package chap06.EX03;

class Animal{
	//1. 필드 
	String animalName; 
	String animalEat; 
	int animalAge; 
	
	//2. 생성자 :  기본 생성자는 다른 생성자가 없을때 생략가능 
	// Animal(){}
	
	//3. 메소드 
	void printAll () {
		System.out.println("동물 이름 : " + animalName + ", 동물 먹이 : " + animalEat + 
				", 동물 나이 : " + animalAge);
	}
	
	
	
}

