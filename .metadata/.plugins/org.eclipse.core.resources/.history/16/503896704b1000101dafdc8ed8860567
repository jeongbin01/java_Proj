package chap04.EX06;

public class Label {

	public static void main(String[] args) {
		// labe (라벨, 레이블)  : 제어 키워드 (continue, break) 를 특정 위치로 점프 해서 이동 함 
		
		// 구구단 출력
		for (int i = 1 ; i < 10 ; i++) {   // 단을 출력 
			System.out.println(i + " 단 입니다");
			System.out.println("============");
			for (int j = 1 ; j < 10 ; j++ ) {
				
				if (i == j) {
					continue;
				}
				
				System.out.println(i + " * " + j + " = " + (i * j));
				
			}
			System.out.println();
			
		}

	}

}
